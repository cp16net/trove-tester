[project]
	description = test bed for trove
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Craig (cp16net)
[access "refs/for/refs/*"]
	read = group user/Craig (cp16net)
	push = group user/Craig (cp16net)
	label-Code-Review = -2..+2 group user/Craig (cp16net)
	label-Verified = -1..+1 group user/Craig (cp16net)
	rebase = group user/Craig (cp16net)
	removeReviewer = group user/Craig (cp16net)
	submit = group user/Craig (cp16net)
[access "refs/heads/*"]
	read = group user/Craig (cp16net)
	create = group user/Craig (cp16net)
	push = +force group user/Craig (cp16net)
	pushMerge = group user/Craig (cp16net)
[access "refs/tags/*"]
	push = +force group user/Craig (cp16net)
	pushTag = group user/Craig (cp16net)
	pushSignedTag = group user/Craig (cp16net)
